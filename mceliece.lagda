dist x z d = Vec‚â§.length $ filter drata $ zip·µ• x' z'
  where
  drata = _‚âü_ false ‚àò isYes ‚àò uncurry _‚âü_
  x' = vec x ‚ñπ_ $ coerce $ cong (Vec _) d
